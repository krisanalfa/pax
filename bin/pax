#!/usr/bin/env node

require('colors');
require('shelljs/global');

var fs = require('fs'),
    CLI = require('../lib/cli').CLI;

(function() {
    "use strict";

    global.DEBUG_START_TIME = (new Date()).getTime();

    var cli = new CLI();

    cli.report = function() {
        var DEBUG_STOP_TIME = (new Date()).getTime();

        console.log('pax elapsed: ' + (DEBUG_STOP_TIME - DEBUG_START_TIME));
    };
    cli.start();

})();